                  </a>
                </div>
              </div>
              <div bind="251138ad-7006-da25-c786-3eddc19a3160" class="swiper w-dyn-list">
                <div bind="251138ad-7006-da25-c786-3eddc19a3161" role="list" class="swiper-wrapper is-product-slider w-dyn-items">
                  {% for product in section.settings.collection_roasts_collection.products limit: section.settings.collection_roasts_collection_limit %}
<div role="listitem" class="swiper-slide is-roasts-slider w-dyn-item">
    
                    
    <a li-object:href="product.url" href="{{ product.url }}" class="link-block w-inline-block">
                      <div class="product-container-image is-isolated"><img class="product-image is-isolated" src="{{ 'product.metafields.custom['isolated_image'].value | img_url: 'master'' | asset_url }}" alt="" sizes="(max-width: 589px) 100vw, 589px" loading="lazy" srcset="{{ 'Hero-Bags-Isolated-Buzz-Armstrong-v1-Top-View-min_1-p-500.png' | asset_url }} 500w, {{ 'Hero-Bags-Isolated-Buzz-Armstrong-v1-Top-View-min_1Hero-Bags-Isolated-Buzz-Armstrong-v1-Top-View-min.webp' | asset_url }} 589w"></div>
                      <div class="product-container-info is-isolated">
                        <div li-object="product.title" class="item-name-small">{{ product.title }}</div>
                        <div class="roast-profile_wrapper is-centered">
                          <div li-object="{{ product.metafields.custom['roast_profile'].value }}" fs-cmsfilter-field="Roast">{{ {{ product.metafields.custom['roast_profile'].value }} }}</div>
                          <div bind="39dfc8e7-977e-8435-b738-6ab13a629869" class="text-block-2">Roast</div>
                        </div>
                      </div>
                    </a>
    
                  
</div>
{% endfor %}
                </div>
                <div bind="251138ad-7006-da25-c786-3eddc19a316a" class="hide w-dyn-empty">
                  <div>No items found.</div>
                </div>
              </div>
              <div class="padding-global-4">
                <div class="container-large-2">
                  <div class="featured-slider_bottom-wrapper">
                    <div class="centered-rule"></div>
                    <div class="swiper-drag-wrapper is-product-sllider">
                      <div class="swiper-drag is-product-slider"></div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div> {% schema %} {
    "tag": "section",

    "settings": [
        {
            "id": "url_button_link",
            "type": "url",
            "label": "Button Link"
        },
        {
            "id": "collection_roasts_collection",
            "type": "collection",
            "label": "Roasts Collection"
        },
        {
            "type": "range",
            "id": "collection_roasts_collection_limit",
            "min": 1,
            "max": 99,
            "step": 1,
            "label": "Limit: Roasts Collection",
            "default": 6
        }
    ],
    "presets": [
        {
            "name": "Home Roasts",
            "category": "Liquify",
            "blocks": []
        }
    ]
} {% endschema %}